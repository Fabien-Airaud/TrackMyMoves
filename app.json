{
  "expo": {
    "name": "TrackMyMoves",
    "slug": "TrackMyMoves",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ic_launcher.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/transparent-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#090909"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/transparent-logo.png",
        "backgroundColor": "#090909"
      }
    },
    "web": {
      "favicon": "./assets/ic_launcher.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
