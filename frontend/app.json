{
  "expo": {
    "name": "TrackMyMoves",
    "slug": "trackmymoves",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ic_launcher.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/transparent-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#090909"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/transparent-logo.png",
        "backgroundColor": "#090909"
      },
      "package": "com.fabien_airaud.trackmymoves"
    },
    "web": {
      "favicon": "./assets/ic_launcher.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8c886f01-0a4d-40c2-9251-c6e2df30cd26"
      }
    }
  }
}
